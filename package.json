{
    "name": "wasmoon",
    "version": "1.9.0",
    "description": "A real lua VM with JS bindings made with webassembly",
    "main": "dist/index.js",
    "scripts": {
        "build:wasm:dev": "./build.sh dev",
        "build:wasm": "./build.sh",
        "start": "rollup -c -w",
        "test": "jest --env=node ./test/",
        "build": "rollup -c && tsc -d --emitDeclarationOnly --declarationDir dist",
        "clean": "rm -rf dist build",
        "lint": "./node_modules/@luvies/config/scripts/lint.sh",
        "fix": "./node_modules/@luvies/config/scripts/lint.sh fix"
    },
    "bin": {
        "wasmoon": "bin/wasmoon"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ceifa/wasmoon.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ceifa/wasmoon/issues"
    },
    "homepage": "https://github.com/ceifa/wasmoon#readme",
    "keywords": [
        "lua",
        "moon",
        "wasm",
        "webassembly"
    ],
    "devDependencies": {
        "@luvies/config": "2.0.4",
        "@rollup/plugin-typescript": "8.2.3",
        "@rollup/plugin-url": "6.0.0",
        "@types/node": "16.4.0",
        "@typescript-eslint/eslint-plugin": "2.34.0",
        "@typescript-eslint/parser": "2.34.0",
        "eslint": "6.8.0",
        "eslint-config-prettier": "6.15.0",
        "eslint-plugin-prettier": "3.3.1",
        "eslint-plugin-sort-imports-es6-autofix": "0.5.0",
        "jest": "27.0.6",
        "prettier": "2.3.2",
        "rollup": "2.53.3",
        "tslib": "2.3.0",
        "typescript": "4.3.5"
    },
    "dependencies": {
        "@types/emscripten": "1.39.5"
    }
}
